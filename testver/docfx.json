{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "index.md",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/index.md",
          "**/*.yml"
        ],
        "src": "netcore1.0",
        "group": "netcore1.0"
      },
      {
        "files": [
          "**/index.md",
          "**/*.yml"
        ],
        "src": "netcore1.1",
        "group": "netcore1.1"
      }      
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "groups": {
       "netcore1.0": {
        "dest": "netcore1.0",
        "moniker_range": "netcore-1.0"
      },
      "netcore1.1": {
        "dest": "netcore1.1",
        "moniker_range": "netcore-1.1"
      }
    },    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/zhen-testver/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "zhen-testver",
    "markdownEngineName": "markdig"
  }
}
